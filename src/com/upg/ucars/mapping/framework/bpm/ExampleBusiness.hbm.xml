<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.upg.ucars.mapping.framework.bpm">
	<class name="ExampleBusiness" table="SYS_EXAMPLE_BUSINESS">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="customerName" column="CUSTOMER_NAME" />
		<property name="arole" column="AROLE" />
		<property name="brole" column="BROLE" />
		<property name="financeAmount" column="FINANCE_AMOUNT" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>