<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.org.OrgAttach" table="org_attach" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="path" type="java.lang.String">
            <column name="path">
                <comment>存放路径，可链接</comment>
            </column>
        </property>
        <property name="fileName" type="java.lang.String">
            <column name="file_name">
                <comment>文件名称</comment>
            </column>
        </property>
        <property name="fileType" type="java.lang.String">
            <column name="file_type">
                <comment>文件类型</comment>
            </column>
        </property>
        <property name="comments" type="java.lang.String">
            <column name="comments">
                <comment>说明</comment>
            </column>
        </property>
        <property name="property" type="java.lang.Short">
            <column name="property">
                <comment>属性附件说明：0基金管理人登记证书，1公司营业执照，2税务登记证，3组织机构代码证，4法人身份证，5法人名片</comment>
            </column>
        </property>
        <property name="orgId" type="java.lang.Long">
            <column name="org_id">
                <comment>机构ID</comment>
            </column>
        </property>
        <property name="createdBy" type="java.lang.Long">
            <column name="created_by" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.Long">
            <column name="last_updated_by" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
        <property name="ctime" type="secDate">
            <column name="ctime" />
        </property>
    </class>
</hibernate-mapping>
