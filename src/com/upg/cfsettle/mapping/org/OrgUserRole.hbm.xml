<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.org.OrgUserRole" table="org_user_role" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="uid" type="java.lang.Integer">
            <column name="uid">
                <comment>用户id</comment>
            </column>
        </property>
        <property name="rid" type="java.lang.Integer">
            <column name="rid">
                <comment>角色id</comment>
            </column>
        </property>
        <property name="createdBy" type="java.lang.Integer">
            <column name="created_by" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.Integer">
            <column name="last_updated_by" />
        </property>
        <property name="ctime" type="java.lang.Integer">
            <column name="ctime" />
        </property>
        <property name="mtime" type="java.lang.Integer">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
