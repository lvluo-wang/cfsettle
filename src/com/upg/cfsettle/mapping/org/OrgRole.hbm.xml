<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.org.OrgRole" table="org_role" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="orgId" type="java.lang.Integer">
            <column name="org_id">
                <comment>机构ID</comment>
            </column>
        </property>
        <property name="roleCode" type="java.lang.String">
            <column name="role_code">
                <comment>角色编码: RXXX，例R009</comment>
            </column>
        </property>
        <property name="roleName" type="java.lang.String">
            <column name="role_name">
                <comment>角色名称</comment>
            </column>
        </property>
        <property name="description" type="java.lang.String">
            <column name="description">
                <comment>角色描述</comment>
            </column>
        </property>
        <property name="isInitial" type="java.lang.Short">
            <column name="is_initial">
                <comment>是否初始角色： 1是， 0否</comment>
            </column>
        </property>
        <property name="createdBy" type="java.lang.Integer">
            <column name="created_by" />
        </property>
        <property name="lastUpdatedBy" type="java.lang.Integer">
            <column name="last_updated_by" />
        </property>
        <property name="ctime" type="java.lang.Integer">
            <column name="ctime" />
        </property>
        <property name="mtime" type="java.lang.Integer">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
