<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsQuestion" table="fi_lcs_question" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="question" type="java.lang.String">
            <column name="question">
                <comment>问题</comment>
            </column>
        </property>
        <property name="typeCode" type="java.lang.String">
            <column name="type_code">
                <comment>类型</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark">
                <comment>备注</comment>
            </column>
        </property>
        <property name="sortNo" type="java.lang.String">
            <column name="sort_no">
                <comment>排序号</comment>
            </column>
        </property>
        <property name="ctime" type="secDate" update="false">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
