<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsFund" table="fi_lcs_fund" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="fundType" type="java.lang.Long">
            <column name="fund_type">
                <comment>基金类型</comment>
            </column>
        </property>
        <property name="fundCompany" type="java.lang.String">
            <column name="fund_company">
                <comment>基金公司</comment>
            </column>
        </property>
        <property name="fundManagent" type="java.lang.String">
            <column name="fund_managent">
                <comment>风险措施</comment>
            </column>
        </property>
        <property name="fundInvestAt" type="java.lang.String">
            <column name="fund_invest_at">
                <comment>基金投向</comment>
            </column>
        </property>
        <property name="fundHighlight" type="java.lang.String">
            <column name="fund_highlight">
                <comment>项目亮点</comment>
            </column>
        </property>
        <property name="fundName" type="java.lang.String">
            <column name="fund_name">
                <comment>产品名称（基金名称）</comment>
            </column>
        </property>
        <property name="status" type="java.lang.Byte">
            <column name="status">
                <comment>审核状态：0未通过，1通过</comment>
            </column>
        </property>
        <property name="fundStatus" type="java.lang.Byte">
            <column name="fund_status">
                <comment>基金状态：1开放期，2封闭期</comment>
            </column>
        </property>
        <property name="timeLimit" type="java.lang.String">
            <column name="time_limit">
                <comment>产品期限</comment>
            </column>
        </property>
        <property name="startTime" type="secDate">
            <column name="start_time">
                <comment>开始时间</comment>
            </column>
        </property>
        <property name="endTime" type="secDate">
            <column name="end_time">
                <comment>结束时间</comment>
            </column>
        </property>
        <property name="scaleAmount" type="java.math.BigDecimal">
            <column name="scale_amount" precision="20" scale="0">
                <comment>产品金额规模</comment>
            </column>
        </property>
        <property name="rate" type="java.lang.String">
            <column name="rate">
                <comment>预期收益</comment>
            </column>
        </property>
        <property name="buildTime" type="secDate" update="false">
            <column name="build_time">
                <comment>成立日期</comment>
            </column>
        </property>
        <property name="fundCode" type="java.lang.String">
            <column name="fund_code">
                <comment>备案编号</comment>
            </column>
        </property>
        <property name="keepBank" type="java.lang.String">
            <column name="keep_bank">
                <comment>托管银行</comment>
            </column>
        </property>
        <property name="partner" type="java.lang.String">
            <column name="partner">
                <comment>合伙人</comment>
            </column>
        </property>
        <property name="manager" type="java.lang.String">
            <column name="manager">
                <comment>管理人</comment>
            </column>
        </property>
        <property name="keeper" type="java.lang.String">
            <column name="keeper">
                <comment>托管人</comment>
            </column>
        </property>
        <property name="netWorth" type="java.lang.String">
            <column name="net_worth">
                <comment>净值</comment>
            </column>
        </property>
        <property name="totalRate" type="java.math.BigDecimal">
            <column name="total_rate" precision="10" scale="2">
                <comment>累计收益率</comment>
            </column>
        </property>
        <property name="oneMonthRate" type="java.math.BigDecimal">
            <column name="one_month_rate" precision="10" scale="2">
                <comment>月收益率</comment>
            </column>
        </property>
        <property name="threeMonthRate" type="java.math.BigDecimal">
            <column name="three_month_rate" precision="10" scale="2">
                <comment>3个月收益率</comment>
            </column>
        </property>
        <property name="currentYearRate" type="java.math.BigDecimal">
            <column name="current_year_rate" precision="10" scale="2">
                <comment>当前年收益率</comment>
            </column>
        </property>
        <property name="minBuyAmount" type="java.lang.String">
            <column name="min_buy_amount">
                <comment>起购金额</comment>
            </column>
        </property>
        <property name="stepBuyAmount" type="java.lang.String">
            <column name="step_buy_amount">
                <comment>递增金额</comment>
            </column>
        </property>
        <property name="subFeeRule" type="java.lang.String">
            <column name="sub_fee_rule">
                <comment>认购费规则</comment>
            </column>
        </property>
        <property name="applyFeeRule" type="java.lang.String">
            <column name="apply_fee_rule">
                <comment>申购费规则</comment>
            </column>
        </property>
        <property name="manageFeeRule" type="java.lang.String">
            <column name="manage_fee_rule">
                <comment>管理费规则</comment>
            </column>
        </property>
        <property name="redeemFeeRule" type="java.lang.String">
            <column name="redeem_fee_rule">
                <comment>赎回费规则</comment>
            </column>
        </property>
        <property name="rewardRule" type="java.lang.String">
            <column name="reward_rule">
                <comment>业绩报酬说明</comment>
            </column>
        </property>
        <property name="investScope" type="java.lang.String">
            <column name="invest_scope">
                <comment>投资范围</comment>
            </column>
        </property>
        <property name="investStrategy" type="java.lang.String">
            <column name="invest_strategy">
                <comment>投资策略</comment>
            </column>
        </property>
        <property name="investRiskCtrl" type="java.lang.String">
            <column name="invest_risk_ctrl">
                <comment>投资风险控制</comment>
            </column>
        </property>
        <property name="top" type="java.lang.Byte">
            <column name="top">
                <comment>置顶标识</comment>
            </column>
        </property>
        <property name="activity" type="java.lang.Byte">
            <column name="activity">
                <comment>活动标识</comment>
            </column>
        </property>
        <property name="ctime" type="secDate" update="false">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
        <property name="rakeBackRate" type="java.math.BigDecimal">
            <column name="rake_back_rate" precision="10" scale="2">
                <comment>返佣比例</comment>
            </column>
        </property>
        <property name="lastReason" type="java.lang.String">
            <column name="last_reason">
                <comment>上次审核意见</comment>
            </column>
        </property>
        <property name="payWay" type="java.lang.String">
            <column name="pay_way">
                <comment>付息方式</comment>
            </column>
        </property>
        <property name="manageOrg" type="java.lang.String">
            <column name="manage_org">
                <comment>管理机构</comment>
            </column>
        </property>
        <property name="publishChannel" type="java.lang.String">
            <column name="publish_channel">
                <comment>发布渠道</comment>
            </column>
        </property>
        <property name="luyanPic" type="java.lang.Long">
            <column name="luyan_pic" >
            	<comment>路演视频图片</comment>
            </column>
        </property>
        <property name="luyanName" type="java.lang.String">
            <column name="luyan_name">
                <comment>材料名称</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
