<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsFundManager" table="fi_lcs_fund_manager" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="fundId" type="java.lang.Long">
            <column name="fund_id" />
        </property>
        <property name="manageType" type="java.lang.Byte">
            <column name="m_type">
                <comment>管理者类型：1基金经理，2基金团队</comment>
            </column>
        </property>
        <property name="xname" type="java.lang.String">
            <column name="xname">
                <comment>姓名</comment>
            </column>
        </property>
        <property name="position" type="java.lang.String">
            <column name="positions">
                <comment>职位</comment>
            </column>
        </property>
        <property name="workYear" type="java.lang.Long">
            <column name="work_year" precision="10" scale="0">
                <comment>工作年限</comment>
            </column>
        </property>
        <property name="avgRate" type="java.lang.Long">
            <column name="avg_rate" precision="10" scale="0">
                <comment>平均收益率</comment>
            </column>
        </property>
        <property name="describe" type="java.lang.String">
            <column name="describes">
                <comment>个人说明</comment>
            </column>
        </property>
        <property name="ctime" type="secDate">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
        <property name="headAttach" type="java.lang.Long">
            <column name="head_attach" />
        </property>
    </class>
</hibernate-mapping>
