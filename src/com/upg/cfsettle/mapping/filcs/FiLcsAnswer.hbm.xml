<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsAnswer" table="fi_lcs_answer" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="questionId" type="java.lang.Long">
            <column name="question_id">
                <comment>问题id</comment>
            </column>
        </property>
        <property name="answer" type="java.lang.String">
            <column name="answer">
                <comment>答案内容</comment>
            </column>
        </property>
        <property name="answerValue" type="java.lang.Long">
            <column name="answer_value">
                <comment>答案分值</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark">
                <comment>答案分值</comment>
            </column>
        </property>
        <property name="sortNo" type="java.lang.String">
            <column name="sort_no">
                <comment>排序号</comment>
            </column>
        </property>
        <property name="ctime" type="secDate" update="false">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
