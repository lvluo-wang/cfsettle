<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsBookingOrder" table="fi_lcs_booking_order" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="orderNo" type="java.lang.String">
            <column name="order_no">
                <comment>订单号</comment>
            </column>
        </property>
        <property name="orderType" type="java.lang.Short">
            <column name="order_type">
                <comment>订单类型：1平台用户订单，2临时用户订单</comment>
            </column>
        </property>
        <property name="fundId" type="java.lang.Long">
            <column name="fund_id">
                <comment>基金产品ID</comment>
            </column>
        </property>
        <property name="amount" type="java.lang.Long">
            <column name="amount" precision="10" scale="0">
                <comment>订单金额</comment>
            </column>
        </property>
        <property name="lcsUid" type="java.lang.Long">
            <column name="lcs_uid">
                <comment>订单用户ID</comment>
            </column>
        </property>
        <property name="fromLcsUid" type="java.lang.Long">
            <column name="from_lcs_uid">
                <comment>理财师ID</comment>
            </column>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status">
                <comment>订单状态：1待处理|2处理中|3成功|4失败</comment>
            </column>
        </property>
        <property name="muserFlag" type="java.lang.Byte">
            <column name="muser_flag" not-null="true">
                <comment>理财师自己添加订单</comment>
            </column>
        </property>
        <property name="dealUid" type="java.lang.Long">
            <column name="deal_uid">
                <comment>处理人ID</comment>
            </column>
        </property>
        <property name="dealRemark" type="java.lang.String">
            <column name="deal_remark">
                <comment>处理备注</comment>
            </column>
        </property>
        <property name="dealTime" type="java.lang.Long">
            <column name="deal_time">
                <comment>处理时间</comment>
            </column>
        </property>
        <property name="ctime" type="secDate">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
