<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.upg.cfsettle.mapping.filcs.FiLcsFundManagerExt" table="fi_lcs_fund_manager_ext" dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="fundManagerId" type="java.lang.Long">
            <column name="fund_manager_id">
                <comment>所属基金管理者id</comment>
            </column>
        </property>
        <property name="fundName" type="java.lang.String">
            <column name="fund_name">
                <comment>基金名称</comment>
            </column>
        </property>
        <property name="startTime" type="secDate">
            <column name="start_time">
                <comment>开始时间</comment>
            </column>
        </property>
        <property name="endTime" type="secDate">
            <column name="end_time">
                <comment>结束时间</comment>
            </column>
        </property>
        <property name="rate" type="java.math.BigDecimal">
            <column name="rate" precision="10" scale="0">
                <comment>收益率</comment>
            </column>
        </property>
        <property name="ctime" type="secDate" update="false">
            <column name="ctime" />
        </property>
        <property name="mtime" type="secDate">
            <column name="mtime" />
        </property>
    </class>
</hibernate-mapping>
