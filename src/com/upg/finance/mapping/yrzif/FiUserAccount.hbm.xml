<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.upg.finance.mapping.yrzif.FiUserAccount" table="fi_user_account">
		<id name="uid" type="long">
			<column name="uid" />
			<generator class="identity" />
		</id>
		<property name="trusteeBank" type="string">
			<column name="trustee_bank" />
		</property>
		<property name="trusteeAccount" type="string">
			<column name="trustee_account" />
		</property>
		<property name="trusteeStatus" type="string">
			<column name="trustee_status" />
		</property>
	</class>
</hibernate-mapping>
